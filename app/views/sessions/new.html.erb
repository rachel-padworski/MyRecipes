<h1>Login Here</h1>

<%= flash[:message] if flash[:message]%>


<%= form_for :user do |f| %>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit "Login" %>
<% end %>
<br><br>
<%= link_to "Log In With Google", '/auth/google_oauth2'%>
<br><br>
<%= link_to "Sign Up Here", signup_path %>
